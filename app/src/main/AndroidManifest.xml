<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jaspreetsingh.travelmania">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TravelMania">
        <activity android:name="com.jaspreetsingh.CovidTracker.OnboardingActivity">

        </activity>
        <activity android:name="com.jaspreetsingh.CovidTracker.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jaspreetsingh.CovidTracker.CountryActivity" />
        <activity android:name="com.jaspreetsingh.CovidTracker.MainActivity" />
    </application>

</manifest>